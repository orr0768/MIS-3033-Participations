<Window x:Class="JSON_Pokemon.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:JSON_Pokemon"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <ListBox x:Name="lstResults" HorizontalAlignment="Left" Height="283" Margin="10,103,0,0" VerticalAlignment="Top" Width="200" SelectionChanged="lstResults_SelectionChanged"/>
        <Image x:Name="imgPokemon" HorizontalAlignment="Left" Height="190" Margin="448,103,0,0" VerticalAlignment="Top" Width="251"/>
        <ListBox x:Name="lstSprites" HorizontalAlignment="Left" Height="283" Margin="249,103,0,0" VerticalAlignment="Top" Width="256"/>

    </Grid>
</Window>
